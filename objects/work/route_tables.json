[
  {
    "name": "work-rt",
    "routes": [
      {
        "name": "monitor-vnet",
        "address_prefix": "${vnet_address_prefixes.monitor}",
        "next_hop_type": "VirtualAppliance",
        "next_hop_in_ip_address": "${private_ip_addresses.firewall}"
      },
      {
        "name": "outbound",
        "address_prefix": "0.0.0.0/0",
        "next_hop_type": "VirtualAppliance",
        "next_hop_in_ip_address": "${private_ip_addresses.firewall}"
      },
      {
        "name": "hub-vnet",
        "address_prefix": "${vnet_address_prefixes.hub}",
        "next_hop_type": "VirtualAppliance",
        "next_hop_in_ip_address": "${private_ip_addresses.firewall}"
      },
      {
        "name": "vpn-address-pool-first-half",
        "address_prefix": "${vpn_address_prefixes.first_half}",
        "next_hop_type": "VirtualAppliance",
        "next_hop_in_ip_address": "${private_ip_addresses.firewall}"
      },
      {
        "name": "vpn-address-pool-second-half",
        "address_prefix": "${vpn_address_prefixes.second_half}",
        "next_hop_type": "VirtualAppliance",
        "next_hop_in_ip_address": "${private_ip_addresses.firewall}"
      }
    ]
  }
]